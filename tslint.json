{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            false
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "class-name": true, 
        "member-access": false
    },
    "rulesDirectory": []
}

